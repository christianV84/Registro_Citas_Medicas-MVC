@model Clinica_UPN_V4._3.Consultorio

@{
    ViewData["Title"] = "Details";
}

<h1>Ver Detalles del Consultorio</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <span @Html.DisplayNameFor(model => model.NumConsultorio)>N° Consultorio</span>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NumConsultorio)
        </dd>
        <dt class = "col-sm-2">
            <span @Html.DisplayNameFor(model => model.EspConsultorio)>Especialización del Consultorio</span>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EspConsultorio)
        </dd>
    </dl>
</div>
<hr />
<div>
    <a asp-action="Edit" asp-route-id="@Model?.NumConsultorio" class="btn btn-secondary">Editar</a> |
    <a asp-action="Index" class="btn btn-success">Regresar a la lista</a>
</div>
