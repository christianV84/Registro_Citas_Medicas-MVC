@model Clinica_UPN_V4._3.Citum

@{
    ViewData["Title"] = "Details";
}

<h1>Ver Detalles de la Cita</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <span @Html.DisplayNameFor(model => model.NumCita)>N° Cita</span>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NumCita)
        </dd>
        <dt class = "col-sm-2">
            <span @Html.DisplayNameFor(model => model.Fecha)>Fecha - Hora</span>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>
        <dt class = "col-sm-2">
            <span @Html.DisplayNameFor(model => model.NumConsultorioNavigation)>Datos del Consultorio</span>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NumConsultorioNavigation.NumConsultorio)
            -
            @Html.DisplayFor(model => model.NumConsultorioNavigation.EspConsultorio)
        </dd>
        <dt class = "col-sm-2">
            <span @Html.DisplayNameFor(model => model.UsuarioMedicoNavigation)>Datos del Médico</span>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsuarioMedicoNavigation.NombreCompletoMedico)
        </dd>
        <dt class = "col-sm-2">
            <span @Html.DisplayNameFor(model => model.UsuarioPacienteNavigation)>Datos del Paciente</span>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsuarioPacienteNavigation.NombreCompleto)
        </dd>
    </dl>
</div>
<hr />
<div>
    <!--<a asp-action="Edit" asp-route-id="@Model?.NumCita" class="btn btn-secondary">Editar</a> |-->
    <a asp-action="Index" class="btn btn-success">Regresar a la lista</a>
</div>
